{
  "name": "better-campus",
  "description": "Browser-extension with nice looking skin for fhstp ecampus",
  "version": "2.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ecampus.fhstp.ac.at/*"
      ],
      "js": [
        "override.js"
      ]
    }
  ],
  "action": {
  },
  "permissions": [
  ],
  "web_accessible_resources": [
    {
      "resources": [ "*" ],
      "matches": [ "https://ecampus.fhstp.ac.at/*" ]
    }
  ]
}
